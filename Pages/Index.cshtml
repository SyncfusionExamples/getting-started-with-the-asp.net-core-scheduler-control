@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    List<AppointmentData> appData = new List<AppointmentData>();
    appData.Add(new AppointmentData
    {
        Id = 1,
        Subject = "Weekly Meeting",
        StartTime = new DateTime(2023, 12, 6, 9, 30, 0),
        EndTime = new DateTime(2023, 12, 6, 11, 0, 0)
    });
}

<ejs-schedule id="schedule"
    currentView="Month"
    selectedDate="new DateTime(2023, 11, 15)"
    startHour="09:00"
    endHour="18:00">
    <e-schedule-eventsettings datasource="appData"></e-schedule-eventsettings>
    <e-schedule-views>
        <e-schedule-view option=Month showWeekend="false"></e-schedule-view>
        <e-schedule-view option=Day dateFormat="dd-MMM-yy"></e-schedule-view>
    </e-schedule-views>
</ejs-schedule>